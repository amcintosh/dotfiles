[user]
	name = Andrew McIntosh
	email = EMAIL
	signingkey = SIGNINGKEY

[alias]
	st = status
	co = checkout

[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git lfs smudge %f
	required = true

[commit]
	gpgsign = true
[init]
	defaultBranch = main
